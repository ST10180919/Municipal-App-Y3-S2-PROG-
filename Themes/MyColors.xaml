<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="RaisinBlack">#262221</Color>
    <SolidColorBrush x:Key="RaisinBlackBrush" Color="{StaticResource RaisinBlack}" />

    <Color x:Key="Jet">#3A3535</Color>
    <SolidColorBrush x:Key="JetBrush" Color="{StaticResource Jet}" />

    <Color x:Key="TextWhite">#F6F1ED</Color>
    <SolidColorBrush x:Key="TextWhiteBrush" Color="{StaticResource TextWhite}" />

</ResourceDictionary>