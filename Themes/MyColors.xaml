<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="RaisinBlack">#262221</Color>
    <SolidColorBrush x:Key="RaisinBlackBrush" Color="{StaticResource RaisinBlack}" />

    <Color x:Key="Jet">#3A3535</Color>
    <SolidColorBrush x:Key="JetBrush" Color="{StaticResource Jet}" />

    <Color x:Key="TextWhite">#FBF7F4</Color>
    <SolidColorBrush x:Key="TextWhiteBrush" Color="{StaticResource TextWhite}" />

    <Color x:Key="Platinum">#E1DFDE</Color>
    <SolidColorBrush x:Key="PlatinumBrush" Color="{StaticResource Platinum}" />

    <SolidColorBrush x:Key="TransparentBrush" Color="Transparent" />

    <Color x:Key="ShamrockGreen">#329B67</Color>
    <SolidColorBrush x:Key="GreenBrush" Color="{StaticResource ShamrockGreen}" />

    <Color x:Key="PapayaWhip">#FFEBCB</Color>
    <SolidColorBrush x:Key="PapayaBrush" Color="{StaticResource PapayaWhip}" />

    <Color x:Key="Flax">#EBD27C</Color>
    <SolidColorBrush x:Key="FlaxBrush" Color="{StaticResource Flax}" />

    <Color x:Key="EventTextLite">#848484</Color>
    <SolidColorBrush x:Key="EventTextLiteBrush" Color="{StaticResource EventTextLite}" />

    <Color x:Key="EerieBlack">#1E1E1E</Color>
    <SolidColorBrush x:Key="EerieBlackBrush" Color="{StaticResource EerieBlack}" />

</ResourceDictionary>