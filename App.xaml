<Application
    x:Class="Municipal_App.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:Municipal_App">
    <Application.Resources>
        <ResourceDictionary>
            <FontFamily x:Key="Roboto">/Fonts/#Roboto</FontFamily>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Themes/MyColors.xaml" />
                <ResourceDictionary Source="Themes/DefaultThemes.xaml" />
                <ResourceDictionary Source="Themes/Inputs.xaml" />
                <ResourceDictionary Source="Themes/Buttons.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
